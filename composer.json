{
    "name": "litermi/external-request",
    "description": "The External Request is a package to send request to others api-rest .",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "log",
        "database"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/http": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/config": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "litermi/simple-notification": "^1.0",
        "litermi/logs": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Litermi\\ExternalRequest\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Litermi\\ExternalRequest\\Providers\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Cirel Ramos",
            "email": "cirelramos@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
